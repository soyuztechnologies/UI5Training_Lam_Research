<spiderman:View xmlns:f="sap.ui.layout.form" xmlns:spiderman="sap.ui.core.mvc" xmlns="sap.m" 
                controllerName="mickey.controller.MyView">
    <!-- <Input id="myField" />
    <Button text="Click My Btn" press="onClick" /> -->
    <f:SimpleForm editable="true">
        <f:content>
            <Label text="EmpId" />
            <Input id="idEmpId" value="{/empStr/empId}"/>
            <Label text="EmpName" />
            <Input id="idEmpName" value="{/empStr/empName}"/>
            <Label text="Salary" />
            <Input id="idSal" value="{/empStr/salary}"/>
            <Label text="Currency" />
            <Input id="idCurr" value="{/empStr/currency}"/>
            <Label />
            <Button text="Flip Flop" press="onFlip" />
            <Button text="Print" press="onPrint" />
            <Button text="Change" press="onChange" />
        </f:content>
    </f:SimpleForm>
</spiderman:View>
